	Doi = {10.1016/j.tree.2008.08.003},
	File = {{Trends in Ecology and Evolution 2008 Nathan-1.pdf:/Users/pedro/Documents/Library.papers3/Articles/2008/Nathan/Trends in Ecology and Evolution 2008 Nathan-1.pdf:application/pdf}},
