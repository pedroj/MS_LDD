@article{Moran:2012kj,
	Affiliation = {National Institute for Mathematical and Biological Synthesis, University of Tennessee, Knoxville, Tennessee, United States of America. moranev@nimbios.org},
	Author = {Moran, Emily V and Clark, James S},
	Date-Added = {2016-06-14 11:28:31 +0000},
