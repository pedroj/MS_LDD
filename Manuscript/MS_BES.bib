@article{Gerber:2014je,
	Affiliation = {BIOGECO, UMR1202, INRA, Cestas, France ; BIOGECO, UMR1202, University of Bordeaux, Talence, France.},
	Author = {Gerber, Sophie and Chad{\oe}uf, Jo{\"e}l and Gugerli, Felix and Lascoux, Martin and Buiteveld, Joukje and Cottrell, Joan and Dounavi, Aikaterini and Fineschi, Silvia and Forrest, Laura L and Fogelqvist, Johan and Goicoechea, Pablo G and Jensen, Jan Svejgaard and Salvini, Daniela and Vendramin, Giovanni G and Kremer, Antoine},
