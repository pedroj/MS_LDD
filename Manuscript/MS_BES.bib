@article{Whitney:1998_E31.5487,
	Author = {Whitney, K D and Fogiel, M K and Lamperti, A M and Holbrook, K M and Stauffer, D J and Hardesty, B D and Parker, V T and Smith, T B},
	Date-Added = {2016-06-14 11:28:31 +0000},
