@article{Torimaru:2007cv,
	Affiliation = {Nagoya Univ, Grad Sch Bioagr Sci, Lab Forest Ecol {\&} Physiol, Nagoya, Aichi 4648601, Japan},
	Author = {Torimaru, T and Tani, N and Tsumura, Y and Nishimura, N and Tomaru, N},
	Date-Added = {2016-06-14 11:28:31 +0000},
