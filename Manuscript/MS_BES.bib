	Journal = {Molecular Ecology},
	Language = {English},
	Local-Url = {file://localhost/Users/pedro/Documents/Library.papers3/Articles/2007/Garcia/Molecular%20Ecology%202007%20Garcia.pdf},
	Month = jan,
	Pages = {1947--1955},
	Rating = {0},
	Read = {Yes},
	Title = {{Contemporary pollen and seed dispersal in a Prunus mahaleb population: patterns in distance and direction}},
