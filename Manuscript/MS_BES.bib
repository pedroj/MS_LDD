@article{Katul:2005wa,
	Affiliation = {Duke Univ, Nicholas Sch Environm {\&} Earth Sci, Durham, NC 27708 USA},
	Author = {Katul, GG and Porporato, A and Nathan, R and Siqueira, M and Soons, MB and Poggi, D and Horn, HS and Levin, SA},
	Date-Added = {2016-06-14 11:28:31 +0000},
	Date-Modified = {2016-06-14 11:28:31 +0000},
