	File = {{Divers Distrib 2005 Nathan.pdf:/Users/pedro/Documents/Library.papers3/Articles/2005/Nathan/Divers Distrib 2005 Nathan.pdf:application/pdf;Divers Distrib 2005 Nathan.pdf:/Users/pedro/Documents/Library.papers3/Articles/2005/Nathan/Divers Distrib 2005 Nathan.pdf:application/pdf}},
	Journal = {Diversity and Distributions},
