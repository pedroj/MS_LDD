@article{Carlo:2013vd,
	Affiliation = {Department of Biology and Intercollege Graduate Degree Program in Ecology, Pennsylvania State University, University Park, Pennsylvania 16802, USA. tac17@psu.edu},
	Author = {Carlo, Tom{\'a}s A and Garc{\'\i}a, Daniel and Mart{\'\i}nez, Daniel and Gleditsch, Jason M and Morales, Juan M},
	Date-Added = {2016-06-28 17:32:25 +0000},
	Date-Modified = {2016-06-28 17:32:25 +0000},
	File = {{Ecology 2013 Carlo.pdf:/Users/pedro/Documents/Library.papers3/Articles/2013/Carlo/Ecology 2013 Carlo.pdf:application/pdf}},
	Journal = {Ecology},
	Language = {English},
	Local-Url = {file://localhost/Users/pedro/Documents/Library.papers3/Articles/2013/Carlo/Ecology%202013%20Carlo.pdf},
	Month = mar,
	Number = {2},
	Pages = {301--307},
	Pmid = {23691649},
	Rating = {0},
	Title = {{Where do seeds go when they go far? Distance and directionality of avian seed dispersal in heterogeneous landscapes.}},
	Uri = {\url{papers3://publication/uuid/7D882FD5-B3FB-41E3-8013-24A3A8788AA2}},
	Url = {https://homes.bio.psu.edu/people/faculty/tac17/Site/Publications_files/Carlo%20et%20al.%202013.pdf},
	Volume = {94},
	Year = {2013},
	Abstract = {Seed dispersal at large scales strongly influences plant population dynamics. Still, ecologists have rarely measured seed dispersal at relevant scales, and the role of habitat types in affecting seed dispersal at long distances remains unexplored. We studied seed dispersal of Ilex aquifolium and Crataegus monogyna in northern Spain, hypothesizing that seeds would be recovered at higher rates and at longer distances (LDD) at habitats with fleshy-fruited trees, compared to habitats with other tree types or at open habitats. We tracked seeds in eight landscapes by enriching trees with 15N isotopes at the center of landscapes, and then detected 15N-marked seeds by sampling at distances of up to 700 m. We found that seeds arrive in greater densities and at longer distances in habitats with trees, particularly fleshy-fruited types, producing different LDD probabilities for each habitat. Results also show a disproportional arrival of seeds in habitats similar to those of mother plants, which should affect seed establishment and the genetic diversity of plant neighborhoods. Findings reveal the strong dependence of seed dispersal on the existing templates that guide the movements of avian dispersers in heterogeneous landscapes and also suggest that LDD above tree lines and beyond hard habitat edges can be difficult.},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://homes.bio.psu.edu/people/faculty/tac17/Site/Publications_files/Carlo%20et%20al.%202013.pdf}}

